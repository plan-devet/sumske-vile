{
  "name": "webpack",
  "description": "Include Bootstrap's source Sass and individual JavaScript plugins with Webpack.",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack build --config webpack.prod.js",
    "prod": "webpack build --config webpack.prod.js",
    "dev": "webpack build --config webpack.dev.js",
    "push": "git push && git subtree push --prefix dist origin gh-pages",
    "clean": "rm dist/*.js dist/*.html dist/*.css dist/*.txt dist/*.woff*",
    "start-prod": "webpack serve --config webpack.prod.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  }
}
